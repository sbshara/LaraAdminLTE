{
    "name": "sbshara/laraadminlte",
    "description": "a laravel optimized AdminLTE template ready to use out of the box package",
    "keywords": [
        "laravel", 
        "AdminLTE", 
        "HTML", 
        "template",
        "bootstrap",
        "package",
        "library",
        "ready",
        "easy"
    ],
    
    "license": "MIT",
    
    "authors": [
      {
        "name": "Shiblie Bshara",
        "email": "shiblie@bshara.com"
      }
    ],
    
    "autoload": {
      "psr-4": {
        "sbshara\\laraadminlte\\": "src/"
      }
    },
    
    "autoload-dev": {
      "classmap": [
        "tests/test.php"
      ]
    },
    
    "extra": {
        "laravel": {
            "providers": [
                "sbshara\\laraadminlte\\ServiceProvider"
            ]
        }
    },
    
    "require": {
      "laravel/framework": "~5.6.0|~5.7.0",
      "php": ">=7.0"
    },
    
    "require-dev": {
      "phpunit/phpunit": "~4.0"
    },

    "suggest": {
      "sbshara/breadcrumbs": "Ready out of the box lib to be installed on Laravel"
    }
  }